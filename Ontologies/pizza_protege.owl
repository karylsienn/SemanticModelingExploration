    <!-- http://www.semanticweb.org/karolniewiadomski/ontologies/2022/8/pizza_ontology#AmericanHotPizza -->

    <owl:Class rdf:about="http://www.semanticweb.org/karolniewiadomski/ontologies/2022/8/pizza_ontology#AmericanHotPizza">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/karolniewiadomski/ontologies/2022/8/pizza_ontology#NamedPizza"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/karolniewiadomski/ontologies/2022/8/pizza_ontology#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/karolniewiadomski/ontologies/2022/8/pizza_ontology#JalapenoPepperTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/karolniewiadomski/ontologies/2022/8/pizza_ontology#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/karolniewiadomski/ontologies/2022/8/pizza_ontology#MozzarellaTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/karolniewiadomski/ontologies/2022/8/pizza_ontology#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/karolniewiadomski/ontologies/2022/8/pizza_ontology#PepperoniTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/karolniewiadomski/ontologies/2022/8/pizza_ontology#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/karolniewiadomski/ontologies/2022/8/pizza_ontology#TomatoTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/karolniewiadomski/ontologies/2022/8/pizza_ontology#hasTopping"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.semanticweb.org/karolniewiadomski/ontologies/2022/8/pizza_ontology#JalapenoPepperTopping"/>
                            <rdf:Description rdf:about="http://www.semanticweb.org/karolniewiadomski/ontologies/2022/8/pizza_ontology#MozzarellaTopping"/>
                            <rdf:Description rdf:about="http://www.semanticweb.org/karolniewiadomski/ontologies/2022/8/pizza_ontology#PepperoniTopping"/>
                            <rdf:Description rdf:about="http://www.semanticweb.org/karolniewiadomski/ontologies/2022/8/pizza_ontology#TomatoTopping"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>A pizza that only has mozarella, tomato, pepperoni and jalapeno toppings</rdfs:comment>
    </owl:Class>
    


