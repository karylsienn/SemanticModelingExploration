                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/karolniewiadomski/ontologies/2022/8/pizza_ontology#hasTopping"/>
                        <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/karolniewiadomski/ontologies/2022/8/pizza_ontology#CheeseTopping"/>
                    </owl:Restriction>
                </owl:intersectionOf>
