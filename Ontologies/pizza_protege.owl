    <!-- http://www.semanticweb.org/karolniewiadomski/ontologies/2022/8/pizza_ontology#CheesyPizza -->

    <owl:Class rdf:about="http://www.semanticweb.org/karolniewiadomski/ontologies/2022/8/pizza_ontology#CheesyPizza">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
