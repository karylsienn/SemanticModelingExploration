            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/karolniewiadomski/ontologies/2022/8/pizza_ontology#hasTopping"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">4</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.semanticweb.org/karolniewiadomski/ontologies/2022/8/pizza_ontology#CheeseTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
