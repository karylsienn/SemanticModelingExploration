    <!-- http://www.semanticweb.org/karolniewiadomski/ontologies/2022/8/pizza_ontology#ChinesePizza -->

    <owl:Class rdf:about="http://www.semanticweb.org/karolniewiadomski/ontologies/2022/8/pizza_ontology#ChinesePizza">
